<h3> Your contact lists </h3>

<table class="contact_lists">
  <thead>
    <tr>
      <th> Filename </th>
      <th> Owner </th>
      <th> Contact count </th>
      <th> Date Uploaded </th>
    </tr>
  </thead>
  <tbody>
  <% @contact_lists.each do |c| %>
    <tr>
      <td> <%= link_to c.list_file_name, contact_list_path(c) -%>  </td>
      <td> <%= c.owner.name %> </td>
      <td> <%= c.contacts.count %> </td>
      <td> <%= c.created_at.to_s(:short) %> </td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= content_for_admin do %>
  <h6 class="buttons"> <a href="<%= new_contact_list_path %>" class="button">Add a new contact list </a> </h6>
<% end %>

